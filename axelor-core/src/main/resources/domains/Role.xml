<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_3.0.xsd">

  <module name="auth" package="com.axelor.auth.db"/>

  <entity name="Role">
    <![CDATA[
    This object store the roles.
    ]]>
    
    <string name="name" required="true" unique="true" />
    <string name="description" />
    <many-to-many name="permissions" ref="Permission" />
  </entity>

</domain-models>
