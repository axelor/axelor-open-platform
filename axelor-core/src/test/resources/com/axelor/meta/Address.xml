<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views https://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="address-grid" title="Addresses" model="com.axelor.contact.test.db.Address">
    <field name="street" />
    <field name="area" />
    <field name="city" />
    <field name="zip" />
    <field name="country" />
  </grid>

  <form name="address-form" title="Address" model="com.axelor.contact.test.db.Address">
    <panel>
      <field name="street" />
      <field name="area" />
      <field name="city" />
      <field name="zip" />
      <field name="country" />
      <button name="clickMe" title="Click Me!"
        onClick="com.axelor.contact.web.HelloController:about" />
    </panel>
  </form>

</object-views>
