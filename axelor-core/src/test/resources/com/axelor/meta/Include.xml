<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views https://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <form name="contact-form1" title="Contact" model="com.axelor.test.db.Contact">
    <panel>
      <field name="firstName"/>
      <field name="lastName"/>
    </panel>
  </form>

  <form name="contact-form2" title="Contact" model="com.axelor.test.db.Contact">
    <panel-include view="contact-form1"/>
    <panel>
      <field name="email"/>
    </panel>
  </form>

</object-views>
