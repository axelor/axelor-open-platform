{
  "status": 0,
  "data": {
    "view": {
      "viewId": 154,
      "modelId": 8,
      "name": "user-form",
      "title": "User",
      "model": "com.axelor.auth.db.User",
      "type": "form",
      "items": [
        {
          "type": "panel",
          "title": "Overview",
          "items": [
            {
              "type": "field",
              "name": "name",
              "placeholder": "user name",
              "onChange": "action-condition-user-validName",
              "validIf": "name.length >= 2",
              "autoTitle": "Name"
            },
            {
              "type": "field",
              "name": "code",
              "readonlyIf": "id && code == 'admin'",
              "placeholder": "login name",
              "onChange": "action-condition-user-validCode",
              "validIf": "code.length >= 2",
              "autoTitle": "Code"
            },
            {
              "type": "field",
              "name": "group",
              "placeholder": "user group",
              "target": "com.axelor.auth.db.Group",
              "targetName": "name",
              "autoTitle": "Group"
            },
            {
              "type": "field",
              "name": "email",
              "widget": "Email",
              "autoTitle": "Email"
            },
            {
              "type": "field",
              "name": "language",
              "autoTitle": "Language"
            },
            {
              "type": "field",
              "name": "theme",
              "widget": "theme-select",
              "autoTitle": "Theme"
            },
            {
              "type": "panel",
              "stacked": true,
              "items": [
                {
                  "type": "field",
                  "name": "homeAction",
                  "hidden": true,
                  "bind": "{{__actionSelect.name}}",
                  "autoTitle": "Home action"
                },
                {
                  "type": "field",
                  "name": "__actionSelect",
                  "title": "Action",
                  "widget": "SuggestBox",
                  "canNew": "false",
                  "target": "com.axelor.meta.db.MetaAction",
                  "targetName": "name",
                  "domain": "self.home = true"
                }
              ]
            },
            {
              "type": "field",
              "name": "singleTab",
              "widget": "inline-checkbox",
              "autoTitle": "Single tab"
            },
            {
              "type": "field",
              "name": "noHelp",
              "widget": "inline-checkbox",
              "autoTitle": "No help"
            },
            {
              "type": "field",
              "name": "defaultProject",
              "target": "com.axelor.project.db.Project",
              "targetName": "name",
              "autoTitle": "Default project"
            }
          ]
        },
        {
          "type": "panel",
          "title": "Authorization",
          "items": [
            {
              "type": "field",
              "name": "activateOn",
              "validIf": "!activateOn || (activateOn && !expiresOn) || (activateOn && expiresOn && $moment(expiresOn) >= $moment(activateOn))",
              "autoTitle": "Activate on"
            },
            {
              "type": "field",
              "name": "expiresOn",
              "validIf": "!expiresOn || (!activateOn && expiresOn) || (activateOn && expiresOn && $moment(expiresOn) >= $moment(activateOn))",
              "autoTitle": "Expires on"
            },
            {
              "type": "field",
              "name": "blocked",
              "autoTitle": "Blocked"
            },
            {
              "type": "field",
              "name": "forcePasswordChange",
              "autoTitle": "Force password change"
            },
            {
              "type": "field",
              "name": "change",
              "title": "Change password?",
              "showIf": "id && !$readonly()",
              "widget": "boolean"
            },
            {
              "type": "panel",
              "showIf": "change || !id",
              "colSpan": 12,
              "itemSpan": 12,
              "items": [
                {
                  "type": "field",
                  "name": "oldPassword",
                  "title": "Your current password",
                  "help": "Current password of the logged in user",
                  "placeholder": "your current password",
                  "widget": "password",
                  "requiredIf": "change || !id"
                },
                {
                  "type": "panel",
                  "showIf": "id",
                  "itemSpan": 12,
                  "items": [
                    {
                      "type": "field",
                      "name": "newPassword",
                      "title": "New password",
                      "help": "New password of the user",
                      "placeholder": "new password",
                      "widget": "password",
                      "onChange": "action-condition-user-password",
                      "requiredIf": "change || !id",
                      "validIf": "(!change && id) || newPassword.length >= 4"
                    },
                    {
                      "type": "field",
                      "name": "chkPassword",
                      "title": "Confirm new password",
                      "help": "Confirm new password of the user (must be identical to the new password)",
                      "placeholder": "confirm new password",
                      "widget": "password",
                      "requiredIf": "change || !id",
                      "validIf": "chkPassword === newPassword"
                    }
                  ]
                },
                {
                  "type": "panel",
                  "hideIf": "id",
                  "itemSpan": 12,
                  "items": [
                    {
                      "type": "field",
                      "name": "newPassword",
                      "title": "Password",
                      "help": "Password of the new user",
                      "placeholder": "password",
                      "widget": "password",
                      "onChange": "action-condition-user-password",
                      "requiredIf": "change || !id",
                      "validIf": "(!change && id) || newPassword.length >= 4"
                    },
                    {
                      "type": "field",
                      "name": "chkPassword",
                      "title": "Confirm password",
                      "help": "Confirm password of the new user (must be identical to the password)",
                      "placeholder": "confirm password",
                      "widget": "password",
                      "requiredIf": "change || !id",
                      "validIf": "chkPassword === newPassword"
                    }
                  ]
                },
                {
                  "type": "help",
                  "text": "Please use at least 4 characters."
                }
              ]
            }
          ]
        },
        {
          "type": "panel-tabs",
          "items": [
            {
              "type": "panel-related",
              "name": "roles",
              "serverType": "MANY_TO_MANY",
              "formView": "role-form",
              "gridView": "role-grid",
