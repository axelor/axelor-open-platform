	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_3.1.xsd">

	<module name="auth" package="com.axelor.auth.db" />
	
	<entity name="User" cachable="true">
